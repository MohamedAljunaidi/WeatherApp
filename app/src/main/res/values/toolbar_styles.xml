<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Widget.AppTheming.Toolbar" parent="Widget.Material3.Toolbar">
        <item name="contentInsetStart">0dp</item>
        <item name="contentInsetEnd">0dp</item>
        <item name="titleTextAppearance">@style/TextAppearance.AppTheming.Toolbar</item>
        <item name="titleCentered">true</item>
        <item name="android:background">?attr/colorSecondary</item>
        <item name="titleTextColor">@color/white</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:colorControlNormal">@color/white</item>
        <item name="navigationIconTint">@color/white</item>

    </style>

    <style name="TextAppearance.AppTheming.Toolbar" parent="TextAppearance.AppTheming.LabelMedium">
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="TextAppearance.AppTheming.LabelMedium" parent="TextAppearance.Material3.LabelMedium">

        <item name="android:textStyle">normal</item>
        <item name="lineHeight">@dimen/_10sdp</item>
        <item name="android:textSize">@dimen/_16ssp</item>
    </style>

</resources>